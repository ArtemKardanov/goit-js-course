<article>
    <h2>{{name}}</h2>
    <div>
        <div>
            <p>Capital: {{capital}}</p>
            <p>Population: {{population}}</p>
            <ul>Languages:
                {{#each languages}}
                <li>{{name}}</li>
                {{/each}}
            </ul>
        </div>
        <img src="{{flag}}" alt="" width="480">
    </div>
</article>